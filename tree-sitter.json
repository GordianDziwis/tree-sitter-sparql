{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "sparql",
      "camelcase": "SPARQL",
      "title": "SPARQL",
      "scope": "source.sparql",
      "file-types": [
        "sparql",
        "rq"
      ],
      "injection-regex": "^sparql$",
      "class-name": "TreeSitterSparql"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "SPARQL grammar for tree-sitter",
    "authors": [
      {
        "name": "Gordian Dziwis"
      }
    ],
    "links": {
      "repository": "https://github.com/BonaBeavis/tree-sitter-sparql"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}